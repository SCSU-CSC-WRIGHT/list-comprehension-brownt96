def lab_3():
    numbers = (input('Enter a list of numbers: '))
    numbers_2 = (input("Enter a second list of numbers: "))
    num_list = numbers.split(' ')
    num_list2 = numbers_2.split(' ')
        
    for i in num_list:
        if i in num_list2:
            num_list2.remove(i)

    print (num_list + num_list2)
            
    end = input("Continue (yes/exit): ")
    if end == ('yes'):
        lab_3()
    elif end == ('exit'):
        print ("okay")

lab_3()
            
